git pull origin master
deploy_type=$1
cd ./static
rm -rf industry
cd ../src/page
yarn industry-java-build
if [ "${deploy_type}" == "remote" ]; then
    echo "deploy remote, deploy_type: ${deploy_type}"
    cd ../../static
    zip -r industry.zip industry
    ls -lh | grep industry
    scp -P 33333 industry.zip root@085a8d1e51b66c57.natapp.cc:/opt/douson/mp-server/static/
    rm -rf industry.zip
    ls -lh | grep industry
    cd ../
else
    echo "deploy local, deploy_type: ${deploy_type}"
    cd ../../
fi
